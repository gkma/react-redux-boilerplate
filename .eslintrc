{
  "extends": ["airbnb", "prettier"],
  "env": {
    "node": true,
    "es6": true,
    "browser": true,
    "jest": true
  },
  "plugins": ["prettier"],
  "rules": {
    "import/extensions": ["js", "jsx"],
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [1]
  }
}
